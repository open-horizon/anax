/* 
Sample node policy that illustrates how to enable privileged execution mode containers to be deployed onto a node.
Set it to the top level because it is a built-in node property.
*/

{
  "properties": [
    {
      "name": "openhorizon.allowPrivileged",
      "value": true
    }
  ],
  "constraints": []
  "deployment": {
    "properties": [],
    "constraints": []
  },
  "management": {
    "properties": [],
    "constraints": []
  }
}
